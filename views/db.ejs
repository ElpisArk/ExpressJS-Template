<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8" />
    <title>DB</title>
    <link rel="stylesheet" href="/css/db.css" />
    <script src="/send_url.js"></script>
  </head>
  <body>
    <div class="wrapper">
      <div class="content">
        <table>
          <tr>
            users
          </tr>
          <tr>
            <th>id</th>
            <th>username</th>
            <th>email</th>
            <th>password</th>
          </tr>
          <% users.forEach((user) => { %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.username %></td>
            <td><%= user.email %></td>
            <td><%= user.password %></td>
          </tr>
          <% }); %>
        </table>
        <table>
          <tr></tr>
            articles
          </tr>
          <tr>
            <th>id</th>
            <th>title</th>
            <th>summary</th>
            <th>content</th>
            <th>category</th>
          </tr>
          <% articles.forEach((article) => { %>
          <tr>
            <td><%= article.id %></td>
            <td><%= article.title %></td>
            <td><%= article.summary %></td>
            <td><%= article.content %></td>
            <td><%= article.category %></td>
          </tr>
          <% }); %>
        </table>
      </div>
    </div>
  </body>
</html>
